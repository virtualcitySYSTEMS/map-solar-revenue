{
  "name": "@vcmap/solar-revenue",
  "version": "3.0.0",
  "description": "Solar revenue calculator",
  "type": "module",
  "main": "src/index.ts",
  "scripts": {
    "prepublishOnly": "vcmplugin build",
    "build": "vcmplugin build",
    "bundle": "vcmplugin bundle",
