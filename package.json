    "@types/jspdf": "^2.0.0",
    "@types/suncalc": "^1.9.2",
    "@types/uuid": "^10.0.0",
    "@vcmap/plugin-cli": "^4.0.2",
    "@vcsuite/eslint-config": "^3.0.8",
    "@vitest/coverage-v8": "^2.1.8",
    "jest-canvas-mock": "^2.5.2",
    "jsdom": "^24.0.0",
    "resize-observer-polyfill": "^1.5.1",
    "typescript": "^5.7.3",
    "vitest": "^2.1.8",
    "vue-tsc": "^2.2.0"
  },
